@model IEnumerable<WebCartridgeJournalAuth.Models.Brand>

@{
    ViewBag.Title = "Index";
}

<h3>Модели картриджей</h3>

<p>
    @Html.ActionLink("Создать новую запись", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.BrandName)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.BrandName)
            </td>
            <td>
                @Html.ActionLink("Правка", "Edit", new { id = item.BrandID }) |
                @Html.ActionLink("Подробности", "Details", new { id = item.BrandID }) |
                @Html.ActionLink("Удалить", "Delete", new { id = item.BrandID })
            </td>
        </tr>
    }

</table>
